CREATE TABLE tutorial.userlogin (
    id text PRIMARY KEY,
    password text
)

CREATE TABLE customer (
    id text PRIMARY KEY,
    billingaddress text,
    name text,
    orderid text,
    phone text
)

CREATE TABLE tutorial.customer (
    id text PRIMARY KEY,
    billingaddress text,
    name text,
    password text,
    phone text
)

